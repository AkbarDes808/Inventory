<Window x:Class="inventory.Delete.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inventory.Delete"
        mc:Ignorable="d"
        Title="Administrator" Height="450" Width="419">
    <Grid Margin="0,0,1,0">
        <TextBox x:Name="Kode" HorizontalAlignment="Left" Margin="156,93,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155" Height="27" TextChanged="Kode_TextChanged"/>
        <Label Content="Kode     :" HorizontalAlignment="Left" Margin="83,94,0,0" VerticalAlignment="Top" Height="35" Width="64"/>
        <Button x:Name="Hapus" Content="Hapus" HorizontalAlignment="Left" Margin="91,130,0,0" VerticalAlignment="Top" Height="37" Width="224" Click="OnClickHapus"/>

    </Grid>
</Window>
