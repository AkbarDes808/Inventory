<Window x:Class="inventory.Read.Admin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inventory.Read"
        mc:Ignorable="d"
        Title="Admin" Height="255" Width="360">
    <Grid Margin="0,0,5,14">
        <Label Content="Kode admin          :" HorizontalAlignment="Left" Margin="62,45,0,0" VerticalAlignment="Top" Width="123"/>
        <Label Content="Nama Admin         :" HorizontalAlignment="Left" Margin="62,83,0,0" VerticalAlignment="Top" Width="127"/>
        <Label Content="no telpon admin   :" HorizontalAlignment="Left" Margin="61,116,0,0" VerticalAlignment="Top" Width="124"/>
        <TextBox x:Name="Nama" HorizontalAlignment="Left" Margin="190,85,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="134" Height="26" TextChanged="Nama_TextChanged"/>
        <TextBox x:Name="Hp" HorizontalAlignment="Left" Margin="190,119,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="134" Height="26" TextChanged="Hp_TextChanged"/>
        <TextBox x:Name="Kode" HorizontalAlignment="Left" Margin="190,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="134" Height="25" TextChanged="Kode_TextChanged"/>

    </Grid>
</Window>
