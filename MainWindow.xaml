        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:inventory"
        mc:Ignorable="d"
        Title="MainWindow" Height="595" Width="326">
    <Grid HorizontalAlignment="Left" Width="589">
        <Label Content="Aplikasi Inventori" HorizontalAlignment="Left" Margin="101,0,0,505" VerticalAlignment="Bottom" Width="128" Height="46"/>
        <Button x:Name="Barang" Content="Input Data" HorizontalAlignment="Left" Margin="90,122,0,0" VerticalAlignment="Top" Height="41" Width="124" Click="OnClickBarang"/>
        <Button x:Name="Lihat" Content="LIhat Data" HorizontalAlignment="Left" Margin="91,194,0,0" VerticalAlignment="Top" Height="46" Width="123" Click="OnClickLihat"/>
        <Button x:Name="Update" Content="Hapus Data" HorizontalAlignment="Left" Margin="91,342,0,0" VerticalAlignment="Top" Height="44" Width="125" Click="OnClickHapus"/>
        <Button Content="Update Data" HorizontalAlignment="Left" Margin="89,270,0,0" VerticalAlignment="Top" Height="46" Width="128" Click="OnClickUpdate"/>
    </Grid>
</Window>
